<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rachitagarwal.stopthebeat" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.rachitagarwal.stopthebeat.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.rachitagarwal.stopthebeat.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.rachitagarwal.stopthebeat.StartActivity"
            android:label="@string/title_activity_start"
            android:parentActivityName="com.rachitagarwal.stopthebeat.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.rachitagarwal.stopbeat.MainActivity" />
        </activity>
    </application>

</manifest>
